generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Account {
  id       String @unique @default(uuid())
  email    String @unique
  name     String
  password String
  token    String? 

  @@map("account")
}

model Log {
  id       String     @unique @default(uuid())
  stack    String 
  date     DateTime   
}
